     procedure_id : 2303332042523538993
     type_code : 14
     code : |1-
      // Summary: Defines the return page to call when accepting the payment
      // Syntax:
      //SetReturnPagePaymentOK (<sReturnPage> [, <sTextContinueButton>])
      //
      // Parameters:
      //	sReturnPage: Name of the WEBDEV page or URL address of the page to display
      //	sTextContinueButton (default value=""): Text of the 'Continue' button on the payment page of PayPal
      // Return value:
      // 	None
      
      procedure public SetReturnPagePaymentOK(sReturnPage, sTextContinueButton = "")
      
      gsPageReturnOK = sReturnPage
      
      if gsPageReturnOK <> "" then
      	gsReturn	= PageAddress("", paFULL) + "?" + BTN_RETURNOK..Alias
      	gsCbt		= sTextContinueButton
      else
      	gsReturn	= ""
      	gsCbt		= ""
      end
      
      _HTML()
